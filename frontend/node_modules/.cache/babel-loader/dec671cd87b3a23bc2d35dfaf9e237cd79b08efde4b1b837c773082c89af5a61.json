{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      drawer: false,\n      drawerItems: [{\n        title: 'DASHBOARD',\n        icon: 'mdi-view-dashboard',\n        to: '/dashboard'\n      }, {\n        title: 'CLIENT TRACKING',\n        icon: 'mdi-account-group',\n        to: '/clients'\n      }, {\n        title: 'AGENT TRACKING',\n        icon: 'mdi-account-tie',\n        to: '/agents'\n      }, {\n        title: 'COMMS. MGMT.',\n        icon: 'mdi-message-text-outline',\n        to: '/commission-management'\n      }, {\n        title: 'SALES TRACKING',\n        icon: 'mdi-chart-line',\n        to: '/sales-tracking'\n      }, {\n        title: 'PROPERTY LISTING',\n        icon: 'mdi-home-city-outline',\n        to: {\n          name: 'PropertyListing'\n        }\n      }],\n      tab: 0,\n      searchQuery: '',\n      properties: [],\n      projects: [],\n      propertyModal: false,\n      isEditMode: false,\n      modalProperty: {},\n      modalListType: 'private',\n      // or 'project'\n      rules: {\n        required: v => !!v || 'This field is required',\n        projectId: v => /^\\d+$/.test(v) || 'ID must be a number',\n        projectName: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        housemodel: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        price: v => /^\\d+$/.test(v) || 'Price must be a number',\n        dateListed: v => /^\\d{4}-\\d{2}-\\d{2}$/.test(v) || 'Date must be YYYY-MM-DD',\n        location: v => /^[a-zA-Z .\\-|]{1,200}$/.test(v) || 'Up to 200 letters, dot, dash, pipe, space',\n        lotArea: v => /^\\d+(\\.\\d{1,2})?$/.test(v) || 'Decimal, up to 2 places',\n        status: v => ['RFO', 'NRFO'].includes(v) || 'Status must be RFO or NRFO',\n        type: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        ownership: v => /^[a-zA-Z .\\-|]{1,100}$/.test(v) || 'Up to 100 letters, dot, dash, pipe, space',\n        ownerFirst: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        ownerLast: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space'\n      }\n    };\n  },\n  created() {\n    this.fetchProperties();\n    this.fetchProjects();\n  },\n  computed: {\n    filteredProperties() {\n      if (!this.searchQuery) return this.properties;\n      const q = this.searchQuery.toLowerCase();\n      return this.properties.filter(p => p.id.toLowerCase().includes(q) || p.ownership.toLowerCase().includes(q) || p.owner_firstname.toLowerCase().includes(q) || p.owner_lastname.toLowerCase().includes(q) || p.price.toLowerCase().includes(q) || p.model.toLowerCase().includes(q) || p.location.toLowerCase().includes(q) || p.lot_area.toLowerCase().includes(q) || p.type.toLowerCase().includes(q) || p.status.toLowerCase().includes(q) || p.date_listed.toLowerCase().includes(q));\n    },\n    filteredProjects() {\n      if (!this.searchQuery) return this.projects;\n      const q = this.searchQuery.toLowerCase();\n      return this.projects.filter(p => p.id.toLowerCase().includes(q) || p.ownership.toLowerCase().includes(q) || p.owner_firstname.toLowerCase().includes(q) || p.owner_lastname.toLowerCase().includes(q) || p.price.toLowerCase().includes(q) || p.model.toLowerCase().includes(q) || p.location.toLowerCase().includes(q) || p.lot_area.toLowerCase().includes(q) || p.type.toLowerCase().includes(q) || p.status.toLowerCase().includes(q) || p.date_listed.toLowerCase().includes(q));\n    }\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem('isAuthenticated');\n      this.$router.push('/login');\n    },\n    openAddPropertyModal() {\n      this.isEditMode = false;\n      this.modalListType = this.tab === 0 ? 'private' : 'project';\n      this.modalProperty = {\n        id: '',\n        ownership: '',\n        owner_firstname: '',\n        owner_lastname: '',\n        price: '',\n        model: '',\n        location: '',\n        lot_area: '',\n        type: '',\n        status: '',\n        date_listed: ''\n      };\n      this.propertyModal = true;\n    },\n    openEditPropertyModal(property, listType) {\n      this.isEditMode = true;\n      this.modalListType = listType;\n      this.modalProperty = {\n        ...property\n      };\n      this.propertyModal = true;\n    },\n    closePropertyModal() {\n      this.propertyModal = false;\n    },\n    fetchProperties() {\n      axios.get('http://localhost:3000/api/private-properties').then(res => {\n        this.properties = res.data;\n      });\n    },\n    fetchProjects() {\n      axios.get('http://localhost:3000/api/property-projects').then(res => {\n        this.projects = res.data;\n      });\n    },\n    saveProperty() {\n      if (this.isEditMode) {\n        // Edit mode\n        if (this.modalListType === 'private') {\n          const idx = this.properties.findIndex(p => p.id === this.modalProperty.id);\n          if (idx !== -1) this.properties.splice(idx, 1, {\n            ...this.modalProperty\n          });\n        } else {\n          const idx = this.projects.findIndex(p => p.id === this.modalProperty.id);\n          if (idx !== -1) this.projects.splice(idx, 1, {\n            ...this.modalProperty\n          });\n        }\n      } else {\n        // Add mode\n        if (this.modalListType === 'private') {\n          axios.post('http://localhost:3000/api/private-properties', this.modalProperty).then(() => {\n            this.fetchProperties();\n            this.propertyModal = false;\n          });\n        } else {\n          axios.post('http://localhost:3000/api/property-projects', this.modalProperty).then(() => {\n            this.fetchProjects();\n            this.propertyModal = false;\n          });\n        }\n      }\n    },\n    deleteProperty(index, listType) {\n      if (confirm('Are you sure you want to delete this property?')) {\n        const id = listType === 'private' ? this.properties[index].id : this.projects[index].id;\n        const url = listType === 'private' ? `http://localhost:3000/api/private-properties/${id}` : `http://localhost:3000/api/property-projects/${id}`;\n        console.log('Deleting:', id, url); // <-- ADD THIS LINE HERE\n        axios.delete(url).then(() => {\n          if (listType === 'private') this.fetchProperties();else this.fetchProjects();\n        }).catch(err => {\n          alert('Delete failed: ' + (err.response?.data?.error || err.message));\n        });\n      }\n    },\n    formatDate(dateStr) {\n      if (!dateStr) return '';\n      const d = new Date(dateStr);\n      if (isNaN(d)) return dateStr;\n      const year = d.getFullYear();\n      const month = String(d.getMonth() + 1).padStart(2, '0');\n      const day = String(d.getDate()).padStart(2, '0');\n      return `${year}-${month}-${day}`;\n    }\n  }\n};","map":{"version":3,"names":["axios","data","drawer","drawerItems","title","icon","to","name","tab","searchQuery","properties","projects","propertyModal","isEditMode","modalProperty","modalListType","rules","required","v","projectId","test","projectName","housemodel","price","dateListed","location","lotArea","status","includes","type","ownership","ownerFirst","ownerLast","created","fetchProperties","fetchProjects","computed","filteredProperties","q","toLowerCase","filter","p","id","owner_firstname","owner_lastname","model","lot_area","date_listed","filteredProjects","methods","logout","localStorage","removeItem","$router","push","openAddPropertyModal","openEditPropertyModal","property","listType","closePropertyModal","get","then","res","saveProperty","idx","findIndex","splice","post","deleteProperty","index","confirm","url","console","log","delete","catch","err","alert","response","error","message","formatDate","dateStr","d","Date","isNaN","year","getFullYear","month","String","getMonth","padStart","day","getDate"],"sources":["C:\\Users\\jann kevin\\Desktop\\ADVENCH PORTALS\\frontend\\src\\components\\PropertyListing.vue"],"sourcesContent":["<template>\n  <v-app>\n    <!-- Navigation Drawer -->\n    <v-navigation-drawer\n      v-model=\"drawer\"\n      app\n      color=\"#f5f5f5\"\n      width=\"260\"\n      class=\"elevation-3 rounded-drawer\"\n    >\n      <!-- Logo and App Name -->\n      <v-sheet color=\"white\" class=\"pa-4 d-flex align-center\" elevation=\"0\">\n        <img src=\"../assets/logo.png\" alt=\"Advench Realty Logo\" style=\"max-width: 48px; margin-right: 12px;\" />\n        <div>\n          <div class=\"font-weight-bold\" style=\"color:#388e3c;\">ADVÐ•NCH</div>\n          <div class=\"text-caption\" style=\"color:#757575;\">REALTY</div>\n        </div>\n        <v-spacer></v-spacer>\n        <v-btn icon @click=\"drawer = false\">\n          <v-icon color=\"grey darken-2\">mdi-close</v-icon>\n        </v-btn>\n      </v-sheet>\n      <v-divider></v-divider>\n\n      <!-- Navigation List -->\n      <v-list dense nav>\n        <v-list-item\n          v-for=\"item in drawerItems\"\n          :key=\"item.title\"\n          :to=\"item.to\"\n          link\n          class=\"drawer-link\"\n          active-class=\"drawer-link--active\"\n          @click=\"drawer = false\"\n        >\n          <v-list-item-icon>\n            <v-icon :color=\"item.iconColor || 'green darken-2'\">{{ item.icon }}</v-icon>\n          </v-list-item-icon>\n          <v-list-item-title class=\"ml-2\">{{ item.title }}</v-list-item-title>\n        </v-list-item>\n      </v-list>\n      <v-divider class=\"my-2\"></v-divider>\n      <v-list dense>\n        <v-list-item link @click=\"logout\" class=\"drawer-link\">\n          <v-list-item-icon>\n            <v-icon color=\"red darken-1\">mdi-logout</v-icon>\n          </v-list-item-icon>\n          <v-list-item-title class=\"ml-2\">LOGOUT</v-list-item-title>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <!-- Header -->\n    <v-main>\n      <v-container fluid class=\"pa-0\" style=\"max-width: 85vw; margin: 0 auto;\">\n        <v-row align=\"center\" justify=\"space-between\" class=\"px-4 mt-4\">\n          <v-col cols=\"auto\" class=\"d-flex align-center\">\n            <img src=\"../assets/logo.png\" alt=\"Advench Realty Logo\" style=\"max-width: 60px; margin-right: 12px;\" />\n            <div>\n              <h2 class=\"text-h6 font-weight-bold mb-0\">ADVÐ•NCH REALTY</h2>\n              <p class=\"text-subtitle-2 mb-0\">AND DEVELOPMENT CO.</p>\n            </div>\n          </v-col>\n          <v-col cols=\"auto\">\n            <v-btn icon @click=\"drawer = true\">\n              <v-icon color=\"green darken-4\">mdi-menu</v-icon>\n            </v-btn>\n          </v-col>\n        </v-row>\n\n        <!-- Title -->\n        <v-row class=\"mt-6 mb-4\">\n          <v-col cols=\"12\">\n            <h1 class=\"text-h4 font-weight-bold\">PROPERTY MANAGEMENT</h1>\n          </v-col>\n        </v-row>\n\n        <!-- Tabs -->\n        <v-row>\n          <v-col cols=\"12\">\n            <v-tabs v-model=\"tab\" background-color=\"green darken-2\" dark>\n              <v-tab>Private Property</v-tab>\n              <v-tab>Property Project</v-tab>\n            </v-tabs>\n          </v-col>\n        </v-row>\n\n        <!-- Search Bar -->\n        <v-row class=\"mb-1\" align=\"center\">\n          <v-col cols=\"12\" md=\"10\">\n            <v-text-field\n              label=\"Search\"\n              outlined\n              dense\n              hide-details\n              v-model=\"searchQuery\"\n              append-inner-icon=\"mdi-magnify\"\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"auto\" class=\"d-flex justify-end\">\n            <v-btn color=\"grey darken-1\" dark style=\"width: 150px;\">Search</v-btn>\n          </v-col>\n        </v-row>\n\n        <!-- Table for Private Property -->\n        <v-row v-show=\"tab === 0\" class=\"pa-0 ma-0\" style=\"width: 100%;\">\n          <v-col cols=\"12\" class=\"pa-0 ma-0\" style=\"width: 100%;\">\n            <div class=\"table-container\">\n              <v-simple-table dense class=\"custom-table\">\n                <template v-slot:default>\n                  <thead>\n                    <tr>\n                      <th class=\"table-header\">ID</th>\n                      <th class=\"table-header\">Ownership</th>\n                      <th class=\"table-header\">Owner Firstname</th>\n                      <th class=\"table-header\">Owner Lastname</th>\n                      <th class=\"table-header\">Price</th>\n                      <th class=\"table-header\">Model</th>\n                      <th class=\"table-header\">Location</th>\n                      <th class=\"table-header\">Lot Area</th>\n                      <th class=\"table-header\">Type</th>\n                      <th class=\"table-header\">Status</th>\n                      <th class=\"table-header\">Date Listed</th>\n                      <th class=\"table-header actions-column\">Actions</th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"(property, index) in filteredProperties\" :key=\"index\" class=\"table-row\">\n                      <td>{{ property.id }}</td>\n                      <td>{{ property.ownership }}</td>\n                      <td>{{ property.owner_firstname }}</td>\n                      <td>{{ property.owner_lastname }}</td>\n                      <td>{{ property.price }}</td>\n                      <td>{{ property.model }}</td>\n                      <td>{{ property.location }}</td>\n                      <td>{{ property.lot_area }}</td>\n                      <td>{{ property.type }}</td>\n                      <td>{{ property.status }}</td>\n                      <td>{{ formatDate(property.date_listed) }}</td>\n                      <td class=\"actions-cell\">\n                        <v-btn icon color=\"green darken-2\" @click=\"openEditPropertyModal(property, 'private')\">\n                          <v-icon>mdi-pencil</v-icon>\n                        </v-btn>\n                        <v-btn icon color=\"red darken-2\" @click=\"deleteProperty(index, 'private')\">\n                          <v-icon>mdi-delete</v-icon>\n                        </v-btn>\n                      </td>\n                    </tr>\n                  </tbody>\n                </template>\n              </v-simple-table>\n            </div>\n          </v-col>\n        </v-row>\n\n        <!-- Table for Property Project -->\n        <v-row v-show=\"tab === 1\" class=\"pa-0 ma-0\" style=\"width: 100%;\">\n          <v-col cols=\"12\" class=\"pa-0 ma-0\" style=\"width: 100%;\">\n            <div class=\"table-container\">\n              <v-simple-table dense class=\"custom-table\">\n                <template v-slot:default>\n                  <thead>\n                    <tr>\n                      <th class=\"table-header\">ID</th>\n                      <th class=\"table-header\">Ownership</th>\n                      <th class=\"table-header\">Owner Firstname</th>\n                      <th class=\"table-header\">Owner Lastname</th>\n                      <th class=\"table-header\">Price</th>\n                      <th class=\"table-header\">Model</th>\n                      <th class=\"table-header\">Location</th>\n                      <th class=\"table-header\">Lot Area</th>\n                      <th class=\"table-header\">Type</th>\n                      <th class=\"table-header\">Status</th>\n                      <th class=\"table-header\">Date Listed</th>\n                      <th class=\"table-header actions-column\">Actions</th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"(property, index) in filteredProjects\" :key=\"index\" class=\"table-row\">\n                      <td>{{ property.id }}</td>\n                      <td>{{ property.ownership }}</td>\n                      <td>{{ property.owner_firstname }}</td>\n                      <td>{{ property.owner_lastname }}</td>\n                      <td>{{ property.price }}</td>\n                      <td>{{ property.model }}</td>\n                      <td>{{ property.location }}</td>\n                      <td>{{ property.lot_area }}</td>\n                      <td>{{ property.type }}</td>\n                      <td>{{ property.status }}</td>\n                      <td>{{ formatDate(property.date_listed) }}</td>\n                      <td class=\"actions-cell\">\n                        <v-btn icon color=\"green darken-2\" @click=\"openEditPropertyModal(property, 'project')\">\n                          <v-icon>mdi-pencil</v-icon>\n                        </v-btn>\n                        <v-btn icon color=\"red darken-2\" @click=\"deleteProperty(index, 'project')\">\n                          <v-icon>mdi-delete</v-icon>\n                        </v-btn>\n                      </td>\n                    </tr>\n                  </tbody>\n                </template>\n              </v-simple-table>\n            </div>\n          </v-col>\n        </v-row>\n\n        <!-- Add Button -->\n        <v-row justify=\"end\" class=\"mt-4 add-button-row\">\n          <v-col cols=\"auto\">\n            <v-btn color=\"grey darken-1\" dark style=\"width: 200px;\" @click=\"openAddPropertyModal\">Add</v-btn>\n          </v-col>\n        </v-row>\n\n        <!-- Add/Edit Property Modal -->\n        <v-dialog v-model=\"propertyModal\" max-width=\"600px\">\n          <v-card>\n            <v-card-title>\n              <span class=\"text-h6\">{{ isEditMode ? 'Edit Property' : 'Add Property' }}</span>\n            </v-card-title>\n            <v-card-text>\n              <v-form ref=\"propertyForm\">\n                <v-row>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.id\" label=\"ID\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.ownership\" label=\"Ownership\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.owner_firstname\" label=\"Owner Firstname\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.owner_lastname\" label=\"Owner Lastname\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.price\" label=\"Price\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.model\" label=\"Model\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.location\" label=\"Location\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.lot_area\" label=\"Lot Area\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.type\" label=\"Type\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.status\" label=\"Status\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"6\">\n                    <v-text-field v-model=\"modalProperty.date_listed\" label=\"Date Listed\"></v-text-field>\n                  </v-col>\n                </v-row>\n              </v-form>\n            </v-card-text>\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn text @click=\"closePropertyModal\">Cancel</v-btn>\n              <v-btn color=\"green darken-2\" dark @click=\"saveProperty\">{{ isEditMode ? 'Save' : 'Add' }}</v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n      </v-container>\n    </v-main>\n\n    <!-- Footer -->\n    <v-footer app color=\"green darken-4\" dark class=\"custom-footer\">\n      <v-container>\n        <span>Copyright Â© 2023. ADVÐ•NCH REALTY and development co. All rights Reserved.</span>\n      </v-container>\n    </v-footer>\n  </v-app>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      drawer: false,\n      drawerItems: [\n        { title: 'DASHBOARD', icon: 'mdi-view-dashboard', to: '/dashboard' },\n        { title: 'CLIENT TRACKING', icon: 'mdi-account-group', to: '/clients' },\n        { title: 'AGENT TRACKING', icon: 'mdi-account-tie', to: '/agents' },\n        { title: 'COMMS. MGMT.', icon: 'mdi-message-text-outline', to: '/commission-management' },\n        { title: 'SALES TRACKING', icon: 'mdi-chart-line', to: '/sales-tracking' },\n        { title: 'PROPERTY LISTING', icon: 'mdi-home-city-outline', to: { name: 'PropertyListing' } },\n      ],\n      tab: 0,\n      searchQuery: '',\n      properties: [],\n      projects: [],\n      propertyModal: false,\n      isEditMode: false,\n      modalProperty: {},\n      modalListType: 'private', // or 'project'\n      rules: {\n        required: v => !!v || 'This field is required',\n        projectId: v => /^\\d+$/.test(v) || 'ID must be a number',\n        projectName: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        housemodel: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        price: v => /^\\d+$/.test(v) || 'Price must be a number',\n        dateListed: v => /^\\d{4}-\\d{2}-\\d{2}$/.test(v) || 'Date must be YYYY-MM-DD',\n        location: v => /^[a-zA-Z .\\-|]{1,200}$/.test(v) || 'Up to 200 letters, dot, dash, pipe, space',\n        lotArea: v => /^\\d+(\\.\\d{1,2})?$/.test(v) || 'Decimal, up to 2 places',\n        status: v => ['RFO', 'NRFO'].includes(v) || 'Status must be RFO or NRFO',\n        type: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        ownership: v => /^[a-zA-Z .\\-|]{1,100}$/.test(v) || 'Up to 100 letters, dot, dash, pipe, space',\n        ownerFirst: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        ownerLast: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n      },\n    };\n  },\n  created() {\n    this.fetchProperties();\n    this.fetchProjects();\n  },\n  computed: {\n    filteredProperties() {\n      if (!this.searchQuery) return this.properties;\n      const q = this.searchQuery.toLowerCase();\n      return this.properties.filter(\n        p =>\n          p.id.toLowerCase().includes(q) ||\n          p.ownership.toLowerCase().includes(q) ||\n          p.owner_firstname.toLowerCase().includes(q) ||\n          p.owner_lastname.toLowerCase().includes(q) ||\n          p.price.toLowerCase().includes(q) ||\n          p.model.toLowerCase().includes(q) ||\n          p.location.toLowerCase().includes(q) ||\n          p.lot_area.toLowerCase().includes(q) ||\n          p.type.toLowerCase().includes(q) ||\n          p.status.toLowerCase().includes(q) ||\n          p.date_listed.toLowerCase().includes(q)\n      );\n    },\n    filteredProjects() {\n      if (!this.searchQuery) return this.projects;\n      const q = this.searchQuery.toLowerCase();\n      return this.projects.filter(\n        p =>\n          p.id.toLowerCase().includes(q) ||\n          p.ownership.toLowerCase().includes(q) ||\n          p.owner_firstname.toLowerCase().includes(q) ||\n          p.owner_lastname.toLowerCase().includes(q) ||\n          p.price.toLowerCase().includes(q) ||\n          p.model.toLowerCase().includes(q) ||\n          p.location.toLowerCase().includes(q) ||\n          p.lot_area.toLowerCase().includes(q) ||\n          p.type.toLowerCase().includes(q) ||\n          p.status.toLowerCase().includes(q) ||\n          p.date_listed.toLowerCase().includes(q)\n      );\n    },\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem('isAuthenticated');\n      this.$router.push('/login');\n    },\n    openAddPropertyModal() {\n      this.isEditMode = false;\n      this.modalListType = this.tab === 0 ? 'private' : 'project';\n      this.modalProperty = {\n        id: '',\n        ownership: '',\n        owner_firstname: '',\n        owner_lastname: '',\n        price: '',\n        model: '',\n        location: '',\n        lot_area: '',\n        type: '',\n        status: '',\n        date_listed: '',\n      };\n      this.propertyModal = true;\n    },\n    openEditPropertyModal(property, listType) {\n      this.isEditMode = true;\n      this.modalListType = listType;\n      this.modalProperty = { ...property };\n      this.propertyModal = true;\n    },\n    closePropertyModal() {\n      this.propertyModal = false;\n    },\n    fetchProperties() {\n      axios.get('http://localhost:3000/api/private-properties')\n        .then(res => { this.properties = res.data; });\n    },\n    fetchProjects() {\n      axios.get('http://localhost:3000/api/property-projects')\n        .then(res => { this.projects = res.data; });\n    },\n    saveProperty() {\n      if (this.isEditMode) {\n        // Edit mode\n        if (this.modalListType === 'private') {\n          const idx = this.properties.findIndex(p => p.id === this.modalProperty.id);\n          if (idx !== -1) this.properties.splice(idx, 1, { ...this.modalProperty });\n        } else {\n          const idx = this.projects.findIndex(p => p.id === this.modalProperty.id);\n          if (idx !== -1) this.projects.splice(idx, 1, { ...this.modalProperty });\n        }\n      } else {\n        // Add mode\n        if (this.modalListType === 'private') {\n          