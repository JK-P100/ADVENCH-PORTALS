.\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        price: v => /^\\d+$/.test(v) || 'Price must be a number',\n        dateListed: v => /^(0[1-9]|1[0-2])\\/(0[1-9]|[12][0-9]|3[01])\\/\\d{4}$/.test(v) || 'Date must be MM/DD/YYYY',\n        location: v => /^[a-zA-Z .\\-|]{1,200}$/.test(v) || 'Up to 200 letters, dot, dash, pipe, space',\n        lotArea: v => /^\\d+(\\.\\d{1,2})?$/.test(v) || 'Decimal, up to 2 places',\n        status: v => !!v || 'Status is required',\n        type: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        ownership: v => /^[a-zA-Z .\\-|]{1,100}$/.test(v) || 'Up to 100 letters, dot, dash, pipe, space',\n        ownerFirst: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n        ownerLast: v => /^[a-zA-Z .\\-|]{1,50}$/.test(v) || 'Up to 50 letters, dot, dash, pipe, space',\n      },\n    };\n  },\n  created() {\n    this.fetchProperties();\n    this.fetchProjects();\n  },\n  computed: {\n    filteredProperties() {\n      if (!this.searchQuery) return this.properties;\n      const q = this.searchQuery.toLowerCase();\n      return this.properties.filter(\n        p =>\n          p.id.toLowerCase().includes(q) ||\n          p.ownership.toLowerCase().includes(q) ||\n          p.owner_firstname.toLowerCase().includes(q) ||\n          p.owner_lastname.toLowerCase().includes(q) ||\n          p.price.toLowerCase().includes(q) ||\n          p.model.toLowerCase().includes(q) ||\n          p.location.toLowerCase().includes(q) ||\n          p.lot_area.toLowerCase().includes(q) ||\n          p.type.toLowerCase().includes(q) ||\n          p.status.toLowerCase().includes(q) ||\n          p.date_listed.toLowerCase().includes(q)\n      );\n    },\n    filteredProjects() {\n      if (!this.searchQuery) return this.projects;\n      const q = this.searchQuery.toLowerCase();\n      return this.projects.filter(\n        p =>\n          p.id.toLowerCase().inc