{"ast":null,"code":"export default {\n  name: 'LoginPage',\n  data() {\n    return {\n      username: '',\n      password: ''\n    };\n  },\n  methods: {\n    onLogin: async function () {\n      if (this.username && this.password) {\n        try {\n          const response = await fetch('http://localhost:3001/login', {\n            method: 'POST',\n            headers: {\n              'Content-Type': 'application/json'\n            },\n            body: JSON.stringify({\n              username: this.username,\n              password: this.password\n            })\n          });\n          const data = await response.json();\n          if (data.success) {\n            localStorage.setItem('isAuthenticated', 'true');\n            this.$router.push('/dashboard');\n          } else {\n            alert(data.message || 'Login failed');\n          }\n        } catch (err) {\n          alert('Server error');\n        }\n      } else {\n        alert('Please enter both username and password');\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","username","password","methods","onLogin","response","fetch","method","headers","body","JSON","stringify","json","success","localStorage","setItem","$router","push","alert","message","err"],"sources":["C:\\Users\\jann kevin\\Desktop\\ADVENCH PORTALS\\frontend\\src\\components\\Login.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-main class=\"login-main\">\n      <v-container fluid class=\"fill-height pa-0\">\n        <v-row no-gutters class=\"fill-height\">\n          <!-- Left side: Company name and slogan at the bottom -->\n          <v-col cols=\"12\" md=\"6\" class=\"left-side d-flex flex-column justify-center align-center pa-0\">\n            <div class=\"company-block\">\n              <img src=\"@/assets/logo.png\" alt=\"Advench Realty Logo\" class=\"company-logo mb-2\" />\n              <div class=\"slogan slogan-tight mb-0\">\"Where Dreams Find a Home.\"</div>\n            </div>\n          </v-col>\n\n          <!-- Right side: Login form, centered and larger -->\n          <v-col cols=\"12\" md=\"6\" class=\"right-side d-flex align-center justify-center pa-0\">\n            <v-card class=\"pa-14 login-card\" max-width=\"\" min-width=\"420\" elevation=\"2\" rounded=\"xl\">\n              <v-card-title class=\"justify-center text-h4 font-weight-bold mb-8\">Log In</v-card-title>\n              <v-card-text>\n                <v-form @submit.prevent=\"onLogin\">\n                  <v-text-field\n                    v-model=\"username\"\n                    label=\"Username\"\n                    prepend-inner-icon=\"mdi-account\"\n                    variant=\"outlined\"\n                    density=\"comfortable\"\n                    class=\"mb-8 big-input\"\n                    required\n                  />\n                  <v-text-field\n                    v-model=\"password\"\n                    label=\"Password\"\n                    prepend-inner-icon=\"mdi-lock\"\n                    type=\"password\"\n                    variant=\"outlined\"\n                    density=\"comfortable\"\n                    class=\"mb-6 big-input\"\n                    required\n                  />\n                  <div class=\"d-flex justify-end mb-8\">\n                    <a href=\"#\" class=\"forgot-link\">Forgot password?</a>\n                  </div>\n                  <v-btn\n                    type=\"submit\"\n                    color=\"#555\"\n                    block\n                    class=\"login-btn big-btn\"\n                    height=\"56\"\n                  >\n                    LOGIN\n                  </v-btn>\n                </v-form>\n              </v-card-text>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      username: '',\n      password: ''\n    }\n  },\n  methods: {\n    onLogin: async function() {\n      if (this.username && this.password) {\n        try {\n          const response = await fetch('http://localhost:3001/login', {\n            method: 'POST',\n            headers: { 'Content-Type': 'application/json' },\n            body: JSON.stringify({ username: this.username, password: this.password })\n          });\n          const data = await response.json();\n          if (data.success) {\n            localStorage.setItem('isAuthenticated', 'true');\n            this.$router.push('/dashboard');\n          } else {\n            alert(data.message || 'Login failed');\n          }\n        } catch (err) {\n          alert('Server error');\n        }\n      } else {\n        alert('Please enter both username and password');\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.login-main {\n  background: #222;\n  min-height: 100vh;\n  height: 100vh;\n  overflow: hidden;\n}\n.left-side {\n  background: #fff;\n  color: #222;\n  min-height: 100vh;\n  height: 100vh;\n  overflow: hidden;\n}\n.right-side {\n  background: #00723a;\n  min-height: 100vh;\n  height: 100vh;\n  overflow: hidden;\n}\n.company-block {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n  height: 100%;\n  margin: 0;\n}\n.company-logo {\n  width: 600px;\n  max-width: 100%;\n  display: block;\n  margin: 0 auto 8px auto;\n}\n.company-name {\n  font-size: 2.3rem;\n  font-weight: bold;\n  letter-spacing: 1px;\n  font-family: 'Montserrat', Arial, sans-serif;\n  text-align: center;\n  margin-bottom: 0;\n}\n.company-sub {\n  font-size: 1.2rem;\n  letter-spacing: 1px;\n  text-align: center;\n  font-family: 'Montserrat', Arial, sans-serif;\n  margin-bottom: 0;\n}\n.slogan {\n  font-size: 1.25rem;\n  font-style: italic;\n  text-align: center;\n  font-family: 'Montserrat', Arial, sans-serif;\n  margin-top: 0.5rem;\n  color: #222;\n}\n.slogan-tight {\n  margin-top: 0.5rem;\n}\n.forgot-link {\n  font-size: 1rem;\n  color: #05723a;\n  text-decoration: underline;\n}\n.login-btn {\n  font-weight: bold;\n  font-size: 1.3rem;\n  background: #555 !important;\n  color: #222 !important;\n  letter-spacing: 1px;\n}\n.login-card {\n  min-width: 420px;\n  min-height: 520px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n.big-input .v-input__control {\n  font-size: 1.2rem;\n  min-height: 56px;\n}\n.big-btn {\n  font-size: 1.3rem !important;\n  height: 56px !important;\n}\n.v-card-title {\n  font-family: 'Montserrat', Arial, sans-serif;\n  font-weight: bold;\n  font-size: 2rem !important;\n}\n.v-label {\n  font-weight: bold;\n  font-size: 1.15rem;\n  font-family: 'Montserrat', Arial, sans-serif;\n}\n</style> "],"mappings":"AA6DA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,EAAE,eAAAA,CAAA,EAAiB;MACxB,IAAI,IAAI,CAACH,QAAO,IAAK,IAAI,CAACC,QAAQ,EAAE;QAClC,IAAI;UACF,MAAMG,QAAO,GAAI,MAAMC,KAAK,CAAC,6BAA6B,EAAE;YAC1DC,MAAM,EAAE,MAAM;YACdC,OAAO,EAAE;cAAE,cAAc,EAAE;YAAmB,CAAC;YAC/CC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;cAAEV,QAAQ,EAAE,IAAI,CAACA,QAAQ;cAAEC,QAAQ,EAAE,IAAI,CAACA;YAAS,CAAC;UAC3E,CAAC,CAAC;UACF,MAAMF,IAAG,GAAI,MAAMK,QAAQ,CAACO,IAAI,CAAC,CAAC;UAClC,IAAIZ,IAAI,CAACa,OAAO,EAAE;YAChBC,YAAY,CAACC,OAAO,CAAC,iBAAiB,EAAE,MAAM,CAAC;YAC/C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;UACjC,OAAO;YACLC,KAAK,CAAClB,IAAI,CAACmB,OAAM,IAAK,cAAc,CAAC;UACvC;QACF,EAAE,OAAOC,GAAG,EAAE;UACZF,KAAK,CAAC,cAAc,CAAC;QACvB;MACF,OAAO;QACLA,KAAK,CAAC,yCAAyC,CAAC;MAClD;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}