{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'AppDashboard',\n  data() {\n    return {\n      drawer: false,\n      stats: {\n        totalClients: 0,\n        totalProperties: 0,\n        totalSales: 0\n      },\n      drawerItems: [{\n        title: 'DASHBOARD',\n        icon: 'mdi-view-dashboard',\n        to: '/dashboard'\n      }, {\n        title: 'CLIENT TRACKING',\n        icon: 'mdi-account-group',\n        to: '/clients'\n      }, {\n        title: 'AGENT TRACKING',\n        icon: 'mdi-account-tie',\n        to: '/agents'\n      }, {\n        title: 'COMMS. MGMT.',\n        icon: 'mdi-message-text-outline',\n        to: '/commission-management'\n      }, {\n        title: 'SALES TRACKING',\n        icon: 'mdi-chart-line',\n        to: '/sales-tracking'\n      }, {\n        title: 'PROPERTY LISTING',\n        icon: 'mdi-home-city-outline',\n        to: {\n          name: 'PropertyListing'\n        }\n      }],\n      statsInterval: null,\n      // for polling\n      chartToggle: 0\n    };\n  },\n  methods: {\n    fetchStats() {\n      axios.get('http://localhost:3000/api/stats') // Replace with your backend stats endpoint\n      .then(response => {\n        this.stats = response.data;\n      }).catch(error => {\n        console.error('Error fetching stats:', error);\n      });\n    },\n    logout() {\n      localStorage.removeItem('isAuthenticated');\n      this.$router.push('/login');\n    }\n  },\n  mounted() {\n    this.fetchStats();\n    // Poll every 10 seconds for real-time updates\n    this.statsInterval = setInterval(this.fetchStats, 10000);\n  },\n  beforeUnmount() {\n    if (this.statsInterval) clearInterval(this.statsInterval);\n  }\n};","map":{"version":3,"names":["axios","name","data","drawer","stats","totalClients","totalProperties","totalSales","drawerItems","title","icon","to","statsInterval","chartToggle","methods","fetchStats","get","then","response","catch","error","console","logout","localStorage","removeItem","$router","push","mounted","setInterval","beforeUnmount","clearInterval"],"sources":["C:\\Users\\jann kevin\\Desktop\\ADVENCH PORTALS\\frontend\\src\\components\\Dashboard.vue"],"sourcesContent":["<template>\n  <v-app>\n    <!-- Navigation Drawer (reuse your existing one) -->\n    <v-navigation-drawer\n      v-model=\"drawer\"\n      app\n      color=\"#f5f5f5\"\n      width=\"260\"\n      class=\"elevation-3 rounded-drawer\"\n    >\n      <!-- Logo and App Name -->\n      <v-sheet color=\"white\" class=\"pa-4 d-flex align-center\" elevation=\"0\">\n        <img src=\"../assets/logo.png\" alt=\"Advench Realty Logo\" style=\"max-width: 48px; margin-right: 12px;\" />\n        <div>\n          <div class=\"font-weight-bold\" style=\"color:#388e3c;\">ADVЕNCH</div>\n          <div class=\"text-caption\" style=\"color:#757575;\">REALTY</div>\n        </div>\n        <v-spacer></v-spacer>\n        <v-btn icon @click=\"drawer = false\">\n          <v-icon color=\"grey darken-2\">mdi-close</v-icon>\n        </v-btn>\n      </v-sheet>\n      <v-divider></v-divider>\n\n      <!-- Navigation List -->\n      <v-list dense nav>\n        <v-list-item\n          v-for=\"item in drawerItems\"\n          :key=\"item.title\"\n          :to=\"item.to\"\n          link\n          class=\"drawer-link\"\n          active-class=\"drawer-link--active\"\n          @click=\"drawer = false\"\n        >\n          <v-list-item-icon>\n            <v-icon :color=\"item.iconColor || 'green darken-2'\">{{ item.icon }}</v-icon>\n          </v-list-item-icon>\n          <v-list-item-title class=\"ml-2\">{{ item.title }}</v-list-item-title>\n        </v-list-item>\n      </v-list>\n      <v-divider class=\"my-2\"></v-divider>\n      <v-list dense>\n        <v-list-item link @click=\"logout\" class=\"drawer-link\">\n          <v-list-item-icon>\n            <v-icon color=\"red darken-1\">mdi-logout</v-icon>\n          </v-list-item-icon>\n          <v-list-item-title class=\"ml-2\">LOGOUT</v-list-item-title>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-main class=\"dashboard-bg\">\n      <v-container fluid>\n        <!-- Greeting and Chart Row -->\n        <v-row>\n          <v-col cols=\"12\" md=\"8\">\n            <v-card class=\"pa-6 chart-card\">\n              <div class=\"d-flex align-center mb-4\">\n                <h2 class=\"font-weight-bold white--text mr-2\">Good Morning,</h2>\n                <span class=\"white--text\" style=\"font-size: 1.5rem;\">Dr Adam.</span>\n              </div>\n              <div class=\"d-flex align-center mb-4\">\n                <span class=\"white--text font-weight-bold\" style=\"font-size: 1.1rem;\">APPOINTMENT STATISTICS</span>\n                <v-spacer></v-spacer>\n                <v-btn-toggle v-model=\"chartToggle\" class=\"toggle-btns\" dense>\n                  <v-btn small depressed color=\"primary\">Monthly</v-btn>\n                  <v-btn small depressed color=\"primary\">Weekly</v-btn>\n                  <v-btn small depressed color=\"primary\">30 Day</v-btn>\n                </v-btn-toggle>\n              </div>\n              <!-- Chart Placeholder -->\n              <div class=\"chart-placeholder\">\n                <v-img src=\"https://dummyimage.com/600x220/1a2340/ffffff&text=Chart+Goes+Here\" contain height=\"220\"></v-img>\n              </div>\n            </v-card>\n          </v-col>\n          <v-col cols=\"12\" md=\"4\">\n            <v-row dense>\n              <v-col cols=\"12\">\n                <v-card class=\"pa-4 info-card\">\n                  <div class=\"d-flex align-center justify-space-between\">\n                    <div>\n                      <div class=\"info-title\">APPROVAL REQUEST</div>\n                      <div class=\"info-value\">26</div>\n                      <div class=\"info-desc\">Request Waiting To Approve</div>\n                    </div>\n                    <v-btn icon color=\"primary\"><v-icon>mdi-arrow-right</v-icon></v-btn>\n                  </div>\n                </v-card>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-card class=\"pa-4 info-card\">\n                  <div class=\"d-flex align-center justify-space-between\">\n                    <div>\n                      <div class=\"info-title\">UPCOMING APPOINTMENT</div>\n                      <div class=\"info-value\">14</div>\n                    </div>\n                    <v-btn icon color=\"primary\"><v-icon>mdi-arrow-right</v-icon></v-btn>\n                  </div>\n                </v-card>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-card class=\"pa-4 info-card\">\n                  <div class=\"d-flex align-center justify-space-between\">\n                    <div>\n                      <div class=\"info-title\">CLINIC INFORMATION</div>\n                      <div class=\"info-desc\">7898 Marsh Ln Undefined Richardson, Wisconsin 35697 United States</div>\n                      <div class=\"info-desc\"><v-icon small class=\"mr-1\">mdi-phone</v-icon> (205) 555-0100 • (262) 555-0131</div>\n                    </div>\n                    <v-btn icon color=\"primary\"><v-icon>mdi-arrow-right</v-icon></v-btn>\n                  </div>\n                </v-card>\n              </v-col>\n            </v-row>\n          </v-col>\n        </v-row>\n\n        <!-- Modules Row (5 modules as cards) -->\n        <v-row class=\"mt-6\" dense>\n          <v-col cols=\"12\" md=\"2\">\n            <v-card class=\"module-card pa-4\">\n              <v-icon size=\"40\" color=\"primary\" class=\"mb-2\">mdi-account-group</v-icon>\n              <div class=\"module-title\">CLIENTS</div>\n              <div class=\"module-value\">2</div>\n              <div class=\"module-desc\">Total Clients</div>\n              <v-btn text color=\"primary\" class=\"mt-2\" @click=\"$router.push('/clients')\">More <v-icon right>mdi-arrow-right</v-icon></v-btn>\n            </v-card>\n          </v-col>\n          <v-col cols=\"12\" md=\"2\">\n            <v-card class=\"module-card pa-4\">\n              <v-icon size=\"40\" color=\"primary\" class=\"mb-2\">mdi-home-city-outline</v-icon>\n              <div class=\"module-title\">PROPERTIES</div>\n              <div class=\"module-value\">5</div>\n              <div class=\"module-desc\">Total Properties</div>\n              <v-btn text color=\"primary\" class=\"mt-2\" @click=\"$router.push('/property-listing')\">More <v-icon right>mdi-arrow-right</v-icon></v-btn>\n            </v-card>\n          </v-col>\n          <v-col cols=\"12\" md=\"2\">\n            <v-card class=\"module-card pa-4\">\n              <v-icon size=\"40\" color=\"primary\" class=\"mb-2\">mdi-currency-usd</v-icon>\n              <div class=\"module-title\">SALES</div>\n              <div class=\"module-value\">$23,000</div>\n              <div class=\"module-desc\">This Month So Far</div>\n              <v-btn text color=\"primary\" class=\"mt-2\" @click=\"$router.push('/sales-tracking')\">More <v-icon right>mdi-arrow-right</v-icon></v-btn>\n            </v-card>\n          </v-col>\n          <v-col cols=\"12\" md=\"2\">\n            <v-card class=\"module-card pa-4\">\n              <v-icon size=\"40\" color=\"primary\" class=\"mb-2\">mdi-account-tie</v-icon>\n              <div class=\"module-title\">AGENTS</div>\n              <div class=\"module-value\">10</div>\n              <div class=\"module-desc\">Total Agents</div>\n              <v-btn text color=\"primary\" class=\"mt-2\" @click=\"$router.push('/agents')\">More <v-icon right>mdi-arrow-right</v-icon></v-btn>\n            </v-card>\n          </v-col>\n          <v-col cols=\"12\" md=\"2\">\n            <v-card class=\"module-card pa-4\">\n              <v-icon size=\"40\" color=\"primary\" class=\"mb-2\">mdi-message-text-outline</v-icon>\n              <div class=\"module-title\">COMMISSION</div>\n              <div class=\"module-value\">$35,000</div>\n              <div class=\"module-desc\">Previous Month</div>\n              <v-btn text color=\"primary\" class=\"mt-2\" @click=\"$router.push('/commission-management')\">More <v-icon right>mdi-arrow-right</v-icon></v-btn>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'AppDashboard',\n  data() {\n    return {\n      drawer: false,\n      stats: {\n        totalClients: 0,\n        totalProperties: 0,\n        totalSales: 0,\n      },\n      drawerItems: [\n        { title: 'DASHBOARD', icon: 'mdi-view-dashboard', to: '/dashboard' },\n        { title: 'CLIENT TRACKING', icon: 'mdi-account-group', to: '/clients' },\n        { title: 'AGENT TRACKING', icon: 'mdi-account-tie', to: '/agents' },\n        { title: 'COMMS. MGMT.', icon: 'mdi-message-text-outline', to: '/commission-management' },\n        { title: 'SALES TRACKING', icon: 'mdi-chart-line', to: '/sales-tracking' },\n        { title: 'PROPERTY LISTING', icon: 'mdi-home-city-outline', to: { name: 'PropertyListing' } },\n      ],\n      statsInterval: null, // for polling\n      chartToggle: 0,\n    };\n  },\n  methods: {\n    fetchStats() {\n      axios\n        .get('http://localhost:3000/api/stats') // Replace with your backend stats endpoint\n        .then((response) => {\n          this.stats = response.data;\n        })\n        .catch((error) => {\n          console.error('Error fetching stats:', error);\n        });\n    },\n    logout() {\n      localStorage.removeItem('isAuthenticated');\n      this.$router.push('/login');\n    },\n  },\n  mounted() {\n    this.fetchStats();\n    // Poll every 10 seconds for real-time updates\n    this.statsInterval = setInterval(this.fetchStats, 10000);\n  },\n  beforeUnmount() {\n    if (this.statsInterval) clearInterval(this.statsInterval);\n  },\n};\n</script>\n\n<style>\n.rounded-drawer {\n  border-top-right-radius: 18px;\n  border-bottom-right-radius: 18px;\n}\n.drawer-link {\n  border-radius: 8px;\n  margin: 2px 4px;\n  transition: background 0.2s;\n}\n.drawer-link:hover {\n  background: #e0f2f1 !important;\n}\n.drawer-link--active {\n  background: #c8e6c9 !important;\n  font-weight: bold;\n}\n.dashboard-bg {\n  background: #f5f8fc;\n  min-height: 100vh;\n}\n.chart-card {\n  background: #1a2340 !important;\n  border-radius: 18px;\n  color: #fff;\n}\n.info-card {\n  background: #fff !important;\n  border-radius: 18px;\n  min-height: 110px;\n  margin-bottom: 16px;\n}\n.info-title {\n  font-size: 1rem;\n  font-weight: 600;\n  color: #1a2340;\n}\n.info-value {\n  font-size: 2.2rem;\n  font-weight: 700;\n  color: #1a2340;\n}\n.info-desc {\n  font-size: 0.95rem;\n  color: #7b7b7b;\n}\n.module-card {\n  background: #fff !important;\n  border-radius: 18px;\n  min-height: 180px;\n  text-align: center;\n  box-shadow: 0 2px 8px #e0e0e0;\n}\n.module-title {\n  font-size: 1.1rem;\n  font-weight: 600;\n  color: #1a2340;\n}\n.module-value {\n  font-size: 2rem;\n  font-weight: 700;\n  color: #1a2340;\n}\n.module-desc {\n  font-size: 0.95rem;\n  color: #7b7b7b;\n}\n.toggle-btns .v-btn {\n  background: #fff !important;\n  color: #1a2340 !important;\n  border-radius: 8px !important;\n  margin-right: 8px;\n}\n.toggle-btns .v-btn--active {\n  background: #1976d2 !important;\n  color: #fff !important;\n}\n.chart-placeholder {\n  background: #22305a;\n  border-radius: 12px;\n  margin-top: 16px;\n  padding: 12px;\n  min-height: 220px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n</style>\n\n"],"mappings":"AA4KA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,KAAK;MACbC,KAAK,EAAE;QACLC,YAAY,EAAE,CAAC;QACfC,eAAe,EAAE,CAAC;QAClBC,UAAU,EAAE;MACd,CAAC;MACDC,WAAW,EAAE,CACX;QAAEC,KAAK,EAAE,WAAW;QAAEC,IAAI,EAAE,oBAAoB;QAAEC,EAAE,EAAE;MAAa,CAAC,EACpE;QAAEF,KAAK,EAAE,iBAAiB;QAAEC,IAAI,EAAE,mBAAmB;QAAEC,EAAE,EAAE;MAAW,CAAC,EACvE;QAAEF,KAAK,EAAE,gBAAgB;Q