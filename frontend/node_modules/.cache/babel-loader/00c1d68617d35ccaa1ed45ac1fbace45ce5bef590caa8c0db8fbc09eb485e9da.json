{"ast":null,"code":"export default {\n  data() {\n    return {\n      drawer: false,\n      drawerItems: [{\n        title: 'DASHBOARD',\n        icon: 'mdi-view-dashboard',\n        to: '/dashboard'\n      }, {\n        title: 'CLIENT TRACKING',\n        icon: 'mdi-account-group',\n        to: '/clients'\n      }, {\n        title: 'AGENT TRACKING',\n        icon: 'mdi-account-tie',\n        to: '/agents'\n      }, {\n        title: 'COMMS. MGMT.',\n        icon: 'mdi-message-text-outline',\n        to: '/commission-management'\n      }, {\n        title: 'SALES TRACKING',\n        icon: 'mdi-chart-line',\n        to: '/sales-tracking'\n      }, {\n        title: 'PROPERTY LISTING',\n        icon: 'mdi-home-city-outline',\n        to: {\n          name: 'PropertyListing'\n        }\n      }],\n      searchQuery: '',\n      modalOpen: false,\n      isEditMode: false,\n      modalCommission: {},\n      editIndex: null,\n      commissionRules: {\n        required: v => !!v || 'This field is required',\n        number: v => /^\\d+(\\.\\d+)?$/.test(v) || 'Must be a number'\n      },\n      commissionFormValid: false\n    };\n  },\n  computed: {\n    filteredCommissions() {\n      if (!this.searchQuery) return this.commissions;\n      const q = this.searchQuery.toLowerCase();\n      return this.commissions.filter(c => c.id.toLowerCase().includes(q) || c.property_no.toLowerCase().includes(q) || c.buyer_name.toLowerCase().includes(q) || c.sales_agent.toLowerCase().includes(q) || c.gross_commission.toLowerCase().includes(q) || c.net_commission.toLowerCase().includes(q) || c.date_disbursed.toLowerCase().includes(q));\n    }\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem('isAuthenticated');\n      this.$router.push('/login');\n    },\n    openAddModal() {\n      this.isEditMode = false;\n      this.modalCommission = {\n        id: '',\n        property_no: '',\n        buyer_name: '',\n        sales_agent: '',\n        gross_commission: '',\n        net_commission: '',\n        date_disbursed: ''\n      };\n      this.modalOpen = true;\n      this.editIndex = null;\n    },\n    openEditModal(commission, index) {\n      this.isEditMode = true;\n      this.modalCommission = {\n        ...commission\n      };\n      this.modalOpen = true;\n      this.editIndex = index;\n    },\n    closeModal() {\n      this.modalOpen = false;\n    },\n    saveCommission() {\n      if (this.$refs.commissionForm.validate()) {\n        if (this.isEditMode && this.editIndex !== null) {\n          this.commissions.splice(this.editIndex, 1, {\n            ...this.modalCommission\n          });\n        } else {\n          this.commissions.push({\n            ...this.modalCommission\n          });\n        }\n        this.modalOpen = false;\n      }\n    },\n    deleteCommission(index) {\n      if (confirm('Are you sure you want to delete this commission?')) {\n        this.commissions.splice(index, 1);\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","drawer","drawerItems","title","icon","to","name","searchQuery","modalOpen","isEditMode","modalCommission","editIndex","commissionRules","required","v","number","test","commissionFormValid","computed","filteredCommissions","commissions","q","toLowerCase","filter","c","id","includes","property_no","buyer_name","sales_agent","gross_commission","net_commission","date_disbursed","methods","logout","localStorage","removeItem","$router","push","openAddModal","openEditModal","commission","index","closeModal","saveCommission","$refs","commissionForm","validate","splice","deleteCommission","confirm"],"sources":["C:\\Users\\jann kevin\\Desktop\\ADVENCH PORTALS\\frontend\\src\\components\\CommissionManagement.vue"],"sourcesContent":["<template>\n  <v-app>\n    <!-- Navigation Drawer -->\n    <v-navigation-drawer\n      v-model=\"drawer\"\n      app\n      color=\"#f5f5f5\"\n      width=\"260\"\n      class=\"elevation-3 rounded-drawer\"\n    >\n      <!-- Logo and App Name -->\n      <v-sheet color=\"white\" class=\"pa-4 d-flex align-center\" elevation=\"0\">\n        <img src=\"../assets/logo.png\" alt=\"Advench Realty Logo\" style=\"max-width: 48px; margin-right: 12px;\" />\n        <div>\n          <div class=\"font-weight-bold\" style=\"color:#388e3c;\">ADVЕNCH</div>\n          <div class=\"text-caption\" style=\"color:#757575;\">REALTY</div>\n        </div>\n        <v-spacer></v-spacer>\n        <v-btn icon @click=\"drawer = false\">\n          <v-icon color=\"grey darken-2\">mdi-close</v-icon>\n        </v-btn>\n      </v-sheet>\n      <v-divider></v-divider>\n\n      <!-- Navigation List -->\n      <v-list dense nav>\n        <v-list-item\n          v-for=\"item in drawerItems\"\n          :key=\"item.title\"\n          :to=\"item.to\"\n          link\n          class=\"drawer-link\"\n          active-class=\"drawer-link--active\"\n          @click=\"drawer = false\"\n        >\n          <v-list-item-icon>\n            <v-icon :color=\"item.iconColor || 'green darken-2'\">{{ item.icon }}</v-icon>\n          </v-list-item-icon>\n          <v-list-item-title class=\"ml-2\">{{ item.title }}</v-list-item-title>\n        </v-list-item>\n      </v-list>\n      <v-divider class=\"my-2\"></v-divider>\n      <v-list dense>\n        <v-list-item link @click=\"logout\" class=\"drawer-link\">\n          <v-list-item-icon>\n            <v-icon color=\"red darken-1\">mdi-logout</v-icon>\n          </v-list-item-icon>\n          <v-list-item-title class=\"ml-2\">LOGOUT</v-list-item-title>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <!-- Header -->\n    <v-main>\n      <v-container fluid class=\"pa-0\" style=\"max-width: 85vw; margin: 0 auto;\">\n        <v-row align=\"center\" justify=\"space-between\" class=\"px-4 mt-4\">\n          <v-col cols=\"auto\" class=\"d-flex align-center\">\n            <img src=\"../assets/logo.png\" alt=\"Advench Realty Logo\" style=\"max-width: 60px; margin-right: 12px;\" />\n            <div>\n              <h2 class=\"text-h6 font-weight-bold mb-0\">ADVЕNCH REALTY</h2>\n              <p class=\"text-subtitle-2 mb-0\">AND DEVELOPMENT CO.</p>\n            </div>\n          </v-col>\n          <v-col cols=\"auto\">\n            <v-btn icon @click=\"drawer = true\">\n              <v-icon color=\"green darken-4\">mdi-menu</v-icon>\n            </v-btn>\n          </v-col>\n        </v-row>\n\n        <!-- Title -->\n        <v-row class=\"mt-6 mb-4\">\n          <v-col cols=\"12\">\n            <h1 class=\"text-h4 font-weight-bold\">COMMISSION MANAGEMENT</h1>\n          </v-col>\n        </v-row>\n\n        <!-- Search Bar -->\n        <v-row class=\"mb-1\" align=\"center\">\n          <v-col cols=\"12\" md=\"10\">\n            <v-text-field\n              label=\"Search\"\n              outlined\n              dense\n              hide-details\n              v-model=\"searchQuery\"\n              append-inner-icon=\"mdi-magnify\"\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"auto\" class=\"d-flex justify-end\">\n            <v-btn color=\"grey darken-1\" dark style=\"width: 150px;\">Search</v-btn>\n          </v-col>\n        </v-row>\n\n        <!-- Table -->\n        <v-row class=\"pa-0 ma-0\" style=\"width: 100%;\">\n          <v-col cols=\"12\" class=\"pa-0 ma-0\" style=\"width: 100%;\">\n            <div class=\"table-container\">\n              <v-simple-table dense class=\"custom-table\">\n                <template v-slot:default>\n                  <thead>\n                    <tr>\n                      <th class=\"table-header\">ID</th>\n                      <th class=\"table-header\">Property No</th>\n                      <th class=\"table-header\">Buyer Name</th>\n                      <th class=\"table-header\">Sales Agent</th>\n                      <th class=\"table-header\">Gross Commission</th>\n                      <th class=\"table-header\">Net Commission</th>\n                      <th class=\"table-header\">Date Disbursed</th>\n                      <th class=\"table-header\">Actions</th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"(commission, index) in filteredCommissions\" :key=\"index\" class=\"table-row\">\n                      <td>{{ commission.id }}</td>\n                      <td>{{ commission.property_no }}</td>\n                      <td>{{ commission.buyer_name }}</td>\n                      <td>{{ commission.sales_agent }}</td>\n                      <td>{{ commission.gross_commission }}</td>\n                      <td>{{ commission.net_commission }}</td>\n                      <td>{{ commission.date_disbursed }}</td>\n                      <td>\n                        <v-btn icon color=\"green darken-2\" @click=\"openEditModal(commission, index)\">\n                          <v-icon>mdi-pencil</v-icon>\n                        </v-btn>\n                        <v-btn icon color=\"red darken-2\" @click=\"deleteCommission(index)\">\n                          <v-icon>mdi-delete</v-icon>\n                        </v-btn>\n                      </td>\n                    </tr>\n                  </tbody>\n                </template>\n              </v-simple-table>\n            </div>\n          </v-col>\n        </v-row>\n\n        <!-- Add Button -->\n        <v-row justify=\"end\" class=\"mt-4 add-button-row\">\n          <v-col cols=\"auto\">\n            <v-btn color=\"grey darken-1\" dark style=\"width: 200px;\" @click=\"openAddModal\">Add</v-btn>\n          </v-col>\n        </v-row>\n\n        <!-- Add/Edit Modal -->\n        <v-dialog v-model=\"modalOpen\" max-width=\"500px\">\n          <v-card>\n            <v-card-title>\n              <span class=\"text-h6\">{{ isEditMode ? 'Edit Commission' : 'Add Commission' }}</span>\n            </v-card-title>\n            <v-card-text>\n              <v-form ref=\"commissionForm\" v-model=\"commissionFormValid\">\n                <v-text-field v-model=\"modalCommission.id\" label=\"ID\" :rules=\"[commissionRules.required]\" required></v-text-field>\n                <v-text-field v-model=\"modalCommission.property_no\" label=\"Property No\" :rules=\"[commissionRules.required]\" required></v-text-field>\n                <v-text-field v-model=\"modalCommission.buyer_name\" label=\"Buyer Name\" :rules=\"[commissionRules.required]\" required></v-text-field>\n                <v-text-field v-model=\"modalCommission.sales_agent\" label=\"Sales Agent\" :rules=\"[commissionRules.required]\" required></v-text-field>\n                <v-text-field v-model=\"modalCommission.gross_commission\" label=\"Gross Commission\" :rules=\"[commissionRules.required, commissionRules.number]\" required></v-text-field>\n                <v-text-field v-model=\"modalCommission.net_commission\" label=\"Net Commission\" :rules=\"[commissionRules.required, commissionRules.number]\" required></v-text-field>\n                <v-text-field v-model=\"modalCommission.date_disbursed\" label=\"Date Disbursed\" :rules=\"[commissionRules.required]\" required></v-text-field>\n              </v-form>\n            </v-card-text>\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn text @click=\"closeModal\">Cancel</v-btn>\n              <v-btn\n                color=\"green darken-2\"\n                dark\n                @click=\"saveCommission\"\n                :disabled=\"!commissionFormValid\"\n              >\n                {{ isEditMode ? 'Save' : 'Add' }}\n              </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n\n        <!-- Back to Dashboard Button -->\n        <v-row>\n          <v-col cols=\"12\">\n            <v-btn\n              color=\"primary\"\n              class=\"mb-4\"\n              @click=\"$router.push({ name: 'AppDashboard' })\"\n              outlined\n            >\n              <v-icon left>mdi-arrow-left</v-icon>\n              Back to Dashboard\n            </v-btn>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-main>\n\n    <!-- Footer -->\n    <v-footer app color=\"green darken-4\" dark class=\"custom-footer\">\n      <v-container>\n        <span>Copyright © 2023. ADVЕNCH REALTY and development co. All rights Reserved.</span>\n      </v-container>\n    </v-footer>\n  </v-app>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      drawer: false,\n      drawerItems: [\n        { title: 'DASHBOARD', icon: 'mdi-view-dashboard', to: '/dashboard' },\n        { title: 'CLIENT TRACKING', icon: 'mdi-account-group', to: '/clients' },\n        { title: 'AGENT TRACKING', icon: 'mdi-account-tie', to: '/agents' },\n        { title: 'COMMS. MGMT.', icon: 'mdi-message-text-outline', to: '/commission-management' },\n        { title: 'SALES TRACKING', icon: 'mdi-chart-line', to: '/sales-tracking' },\n        { title: 'PROPERTY LISTING', icon: 'mdi-home-city-outline', to: { name: 'PropertyListing' } },\n      ],\n      searchQuery: '',\n      \n      modalOpen: false,\n      isEditMode: false,\n      modalCommission: {},\n      editIndex: null,\n      commissionRules: {\n        required: v => !!v || 'This field is required',\n        number: v => /^\\d+(\\.\\d+)?$/.test(v) || 'Must be a number',\n      },\n      commissionFormValid: false,\n    };\n  },\n  computed: {\n    filteredCommissions() {\n      if (!this.searchQuery) return this.commissions;\n      const q = this.searchQuery.toLowerCase();\n      return this.commissions.filter(\n        c =>\n          c.id.toLowerCase().includes(q) ||\n          c.property_no.toLowerCase().includes(q) ||\n          c.buyer_name.toLowerCase().includes(q) ||\n          c.sales_agent.toLowerCase().includes(q) ||\n          c.gross_commission.toLowerCase().includes(q) ||\n          c.net_commission.toLowerCase().includes(q) ||\n          c.date_disbursed.toLowerCase().includes(q)\n      );\n    },\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem('isAuthenticated');\n      this.$router.push('/login');\n    },\n    openAddModal() {\n      this.isEditMode = false;\n      this.modalCommission = {\n        id: '',\n        property_no: '',\n        buyer_name: '',\n        sales_agent: '',\n        gross_commission: '',\n        net_commission: '',\n        date_disbursed: '',\n      };\n      this.modalOpen = true;\n      this.editIndex = null;\n    },\n    openEditModal(commission, index) {\n      this.isEditMode = true;\n      this.modalCommission = { ...commission };\n      this.modalOpen = true;\n      this.editIndex = index;\n    },\n    closeModal() {\n      this.modalOpen = false;\n    },\n    saveCommission() {\n      if (this.$refs.commissionForm.validate()) {\n        if (this.isEditMode && this.editIndex !== null) {\n          this.commissions.splice(this.editIndex, 1, { ...this.modalCommission });\n        } else {\n          this.commissions.push({ ...this.modalCommission });\n        }\n        this.modalOpen = false;\n      }\n    },\n    deleteCommission(index) {\n      if (confirm('Are you sure you want to delete this commission?')) {\n        this.commissions.splice(index, 1);\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.rounded-drawer {\n  border-top-right-radius: 18px;\n  border-bottom-right-radius: 18px;\n}\n.drawer-link {\n  border-radius: 8px;\n  margin: 2px 4px;\n  transition: background 0.2s;\n}\n.drawer-link:hover {\n  background: #e0f2f1 !important;\n}\n.drawer-link--active {\n  background: #c8e6c9 !important;\n}\n.table-container {\n  max-height: 500px;\n  overflow-y: auto;\n  border: 1px solid #ccc;\n}\n.custom-table {\n  width: 100%;\n  border-collapse: separate;\n  border-spacing: 0 10px;\n  table-layout: fixed;\n}\n.table-header {\n  background-color: #4CAF50;\n  color: white;\n  text-align: left;\n  padding: 12px;\n  font-weight: bold;\n  position: sticky;\n  top: 0;\n  z-index: 1;\n}\n.table-row {\n  background-color: #F5F5F5;\n  text-align: left;\n  border-radius: 8px;\n  overflow: hidden;\n}\n.table-row td {\n  padding: 12px 20px;\n  word-wrap: break-word;\n}\n.table-row:hover {\n  background-color: #e0e0e0;\n}\nth, td {\n  text-align: left;\n  width: 8%;\n}\nhtml, body, #app, .v-application {\n  height: 100%;\n  margin: 0;\n}\n.v-app {\n  display: flex;\n  flex-direction: column;\n  min-height: 100%;\n}\n.v-main {\n  flex: 1;\n}\n.custom-footer {\n  background-color: #4CAF50;\n  color: white;\n  height: 50px;\n}\n.add-button-row {\n  margin-bottom: 24px;\n}\n</style>"],"mappings":"AA2MA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE,CACX;QAAEC,KAAK,EAAE,WAAW;QAAEC,IAAI,EAAE,oBAAoB;QAAEC,EAAE,EAAE;MAAa,CAAC,EACpE;QAAEF,KAAK,EAAE,iBAAiB;QAAEC,IAAI,EAAE,mBAAmB;QAAEC,EAAE,EAAE;MAAW,CAAC,EACvE;QAAEF,KAAK,EAAE,gBAAgB;QAAEC,IAAI,EAAE,iBAAiB;QAAEC,EAAE,EAAE;MAAU,CAAC,EACnE;QAAEF,KAAK,EAAE,cAAc;QAAEC,IAAI,EAAE,0BAA0B;QAAEC,EAAE,EAAE;MAAyB,CAAC,EACzF;QAAEF,KAAK,EAAE,gBAAgB;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,EAAE,EAAE;MAAkB,CAAC,EAC1E;QAAEF,KAAK,EAAE,kBAAkB;QAAEC,IAAI,EAAE,uBAAuB;QAAEC,EAAE,EAAE;UAAEC,IAAI,EAAE;QAAkB;MAAE,CAAC,CAC9F;MACDC,WAAW,EAAE,EAAE;MAEfC,SAAS,EAAE,KAAK;MAChBC,UAAU,EAAE,KAAK;MACjBC,eAAe,EAAE,CAAC,CAAC;MACnBC,SAAS,EAAE,IAAI;MACfC,eAAe,EAAE;QACfC,QAAQ,EAAEC,CAAA,IAAK,CAAC,CAACA,CAAA,IAAK,wBAAwB;QAC9CC,MAAM,EAAED,CAAA,IAAK,eAAe,CAACE,IAAI,CAACF,CAAC,KAAK;MAC1C,CAAC;MACDG,mBAAmB,EAAE;IACvB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,mBAAmBA,CAAA,EAAG;MACpB,IAAI,CAAC,IAAI,CAACZ,WAAW,EAAE,OAAO,IAAI,CAACa,WAAW;MAC9C,MAAMC,CAAA,GAAI,IAAI,CAACd,WAAW,CAACe,WAAW,CAAC,CAAC;MACxC,OAAO,IAAI,CAACF,WAAW,CAACG,MAAM,CAC5BC,CAAA,IACEA,CAAC,CAACC,EAAE,CAACH,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,CAAC,KAC7BG,CAAC,CAACG,WAAW,CAACL,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,CAAC,KACtCG,CAAC,CAACI,UAAU,CAACN,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,CAAC,KACrCG,CAAC,CAACK,WAAW,CAACP,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,CAAC,KACtCG,CAAC,CAACM,gBAAgB,CAACR,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,CAAC,KAC3CG,CAAC,CAACO,cAAc,CAACT,WAAW,CA